<Window x:Class="TCPChat_Assync.Cadastro.CadastroScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TCPChat_Assync.Cadastro"
        mc:Ignorable="d"
         WindowStartupLocation="CenterScreen"
        Title="Cadastro Screen" Height="450" Width="800">
    <Grid>
        <Label Content="Nome Completo:" HorizontalAlignment="Left" Margin="265,173,0,0" VerticalAlignment="Top" Height="26" Width="110"/>
        <TextBox x:Name="txtBox_NomeCompleto" HorizontalAlignment="Left" Margin="400,173,0,0" TextWrapping="Wrap" Text="&#xD;&#xA;" VerticalAlignment="Top" Width="158" Height="26"/>
        <Button x:Name="btnVoltar" Content="Voltar" HorizontalAlignment="Left" Margin="320,325,0,0" VerticalAlignment="Top" Height="29" Width="79" Click="btnVoltar_Click"/>
        <TextBox x:Name="txtCadastro" HorizontalAlignment="Center" Margin="0,57,0,0" TextWrapping="Wrap" Text="CADASTRO&#xA;" VerticalAlignment="Top" Width="158" Height="35" FontWeight="Bold" FontSize="20" TextAlignment="Center"/>
        <Label Content="Nome de usuário:" HorizontalAlignment="Left" Margin="265,207,0,0" VerticalAlignment="Top" Height="26" Width="114"/>
        <TextBox x:Name="txtBox_NomeUsuario" HorizontalAlignment="Left" Margin="400,207,0,0" TextWrapping="Wrap" Text="&#xA;" VerticalAlignment="Top" Width="158" Height="26"/>
        <Label Content="Senha:" HorizontalAlignment="Left" Margin="296,239,0,0" VerticalAlignment="Top" Height="26" Width="49"/>
        <TextBox x:Name="txtBox_Senha" HorizontalAlignment="Left" Margin="400,239,0,0" TextWrapping="Wrap" Text="&#xA;" VerticalAlignment="Top" Width="158"  Height="26"/>
        <Button x:Name="btnConcluir" Content="Concluir" HorizontalAlignment="Left" Margin="439,325,0,0" VerticalAlignment="Top" Height="29" Width="80" Click="btnConcluir_Click"/>

    </Grid>
</Window>
